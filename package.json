    "cssnano": "^6.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "husky": "^9.0.7",
    "ky": "^1.2.0",
    "lint-staged": "^15.2.1",
