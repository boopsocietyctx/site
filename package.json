{
  "name": "boop-society-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "full-dev": "doppler run -- astro dev",
    "build": "astro build",
    "local:build": "doppler run -- astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,astro}",
    "prepare": "husky install",
    "pages:secrets": "doppler secrets download --no-file --format env > .dev.vars",
    "pages:dev": "wrangler pages dev --compatibility-date=2024-01-29 -- astro dev",
    "pages:deploy": "astro build && wrangler pages deploy ./dist"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^9.0.0",
