    "@heroicons/react": "^2.1.1",
    "@touch4it/ical-timezones": "^1.9.0",
    "astro": "^4.2.8",
    "astro-seo": "^0.8.0",
    "html-to-text": "^9.0.5",
    "ical-generator": "^6.0.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-responsive": "^9.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240129.0",
    "@tailwindcss/forms": "^0.5.7",
